{
	"name": "Screenplay",
	"version": "0.5",
	"author": [
		"Bartosz DziewoÅ„ski",
		"Calimonius the Estrange"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:Screenplay",
	"descriptionmsg": "screenplay-desc",
	"license-name": "MIT",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
	"type": "parserhook",
	"TrackingCategories": [
		"screenplay-tracking-category"
	],
	"MessagesDirs": {
		"Screenplay": [
			"i18n"
		]
	},
	"config": {
		"ScreenplayTokens": {
			"value": false,
			"path": false,
			"description": "Don't use this.",
			"public": false
		}
	},
	"AutoloadClasses": {
		"ScreenplayParser": "includes/ScreenplayParser.php"
	},
	"ResourceModules": {
		"ext.screenplay": {
			"styles": {
				"0": "ext.screenplay.less",
				"ext.screenplay.print.less": {
					"media": "print"
				}
			},
			"targets": [ "desktop", "mobile" ]
		}
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "Screenplay/resources"
	},
	"Hooks": {
		"ParserFirstCallInit": [
			"ScreenplayParser::init"
		]
	},
	"manifest_version": 2
}
